{
  "name": "@antv/colorlab",
  "version": "1.0.0",
  "description": "Online Color Lab",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx \"./\" && prettier --check '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "fix": "npm run lint -- --fix && prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint-staged": "lint-staged",
    "test": "npm run lint && npm run build",
    "build": "umi build",
    "clean": "rm -rf dist src/.umi",
    "postinstall": "umi generate tmp",
    "prepare": "husky install",
    "deploy": "gh-pages -d dist"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,md,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@antv/color-schema": "^0.2.2",
    "@antv/g2plot": "^2.3.21",
    "@antv/smart-color": "^0.2.0",
    "@types/lodash": "^4.14.172",
    "@umijs/plugin-esbuild": "^1.3.1",
    "ajv": "^8.6.1",
    "antd": "^4.6.6",
    "chroma-js": "^2.1.2",
    "classnames": "^2.2.6",
    "plotly.js-dist": "^1.58.4",
    "react": "^16.3.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-color": "^2.18.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.3.1",
    "react-no-ssr": "^1.1.0",
    "react-slider": "^1.1.4",
    "umi": "^3.5.17",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/chroma-js": "^2.1.3",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-color": "^3.0.4",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-no-ssr": "^1.1.1",
    "@types/react-slider": "^1.1.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.17",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "gh-pages": "^6.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "repository": "https://github.com/antvis/ColorLab.git"
}
